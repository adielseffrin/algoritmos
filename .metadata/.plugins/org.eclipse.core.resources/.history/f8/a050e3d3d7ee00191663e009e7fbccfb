package com.pokedex.model;

public class Pokebola {
	
	private static String geracao = "1";
	private String tipo;
	private Pokemon poke23;
	
	public Pokebola() {
		poke23 = new Pokemon();
		poke23.setNome("Diglet");
	}
	
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public Pokemon getPoke() {
		return poke23;
	}
	public void setPoke(Pokemon poke) {
		System.out.println(geracao);
		this.poke23 = poke;
	}
	
	public static String getGeracao() {
		return geracao;
	}
	
	
}
