package com.pokedex.model;

public class Pokebola {
	
	private static int capacidade = 2;
	private String tipo;
	private Pokemon[] pokes;
	
	public Pokebola() {
		pokes = new Pokemon[capacidade];
	}
	
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public Pokemon getPoke() {
		return poke23;
	}
	public void setPoke(Pokemon poke) {
		System.out.println(geracao);
		this.poke23 = poke;
	}
	
	public static String getGeracao() {
		return geracao;
	}
	
	
}
