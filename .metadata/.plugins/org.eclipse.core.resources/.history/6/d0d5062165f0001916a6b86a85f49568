import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Elemento inicio, fim, aux, novo;
		boolean sair = false;
		Scanner in = new Scanner(System.in);
		int opcao;
		//LDNO
		//inserir no inicio
		//inserir no final
		//buscar um elemento
		//excluir
		//esvaziar
		
		//menu
		do {
			System.out.println("Digite uma opção:\n"
					+ "0 - Sair\n"
					+ "1 - Inserir no inicio\n"
					+ "2 - Inserir no final\n"
					+ "3 - Buscar\n"
					+ "4 - Excluir\n"
					+ "5 - Esvaziar\n");
			opcao = in.nextInt();
			switch (opcao) {
			case 0:
				sair = true;
				break;

			default:
				sair = true;
				break;
			}
		}while(!sair);
		
	}

}
