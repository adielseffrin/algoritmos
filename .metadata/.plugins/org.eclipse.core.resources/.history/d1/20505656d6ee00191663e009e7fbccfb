package com.pokedex.model;

public class Pokebola {
	private String tipo;
	private Pokemon poke;
	
	public Pokebola() {
		poke = new Pokemon();
		pokesurpresa.setNome("Diglet");
	}
	
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public Pokemon getPoke() {
		return poke;
	}
	public void setPoke(Pokemon poke) {
		this.poke = poke;
	}
	
	
}
