package com.pokedex.model;

public class Pokebola {
	
	public int capacidade = 4;
	private String tipo;
	private Pokemon[] pokes;
	
	public Pokebola() {
		pokes = new Pokemon[capacidade];
	}
	
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	
	public Pokemon[] getPokes() {
		return pokes;
	}

	public void setPokes(Pokemon[] pokes) {
		this.pokes = pokes;
	}

	//public static int getCapacidade() {
	//	return capacidade;
	//}
	
	
}
