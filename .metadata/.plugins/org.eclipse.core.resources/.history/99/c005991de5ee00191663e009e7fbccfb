
public class Main {

	public static void main(String[] args) {
		Elemento inicio, fim;
		
		Elemento novo = new Elemento();
		novo.numero = 10;
		
		inicio = novo;
		fim = novo;
		
		novo = new Elemento();
		novo.numero = 5;
		
		inicio.prox = novo;
		fim = novo;
		
		System.out.println(inicio.numero);
		System.out.println(inicio.prox.numero);
		
		novo = new Elemento();
		novo.numero = 36;
		
		novo.prox = inicio.prox; // = fim;
		inicio.prox = novo;
		
		System.out.println(inicio.numero);
		System.out.println(inicio.prox.numero);
		System.out.println(inicio.prox.prox.numero);
		
	}

}
